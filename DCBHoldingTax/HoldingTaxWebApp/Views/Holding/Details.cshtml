@section Scripts {
    <script src="~/Scripts/printThis.js"></script>
    <script>
        jQuery(document).ready(function () {
            $("#print_this").on('click', function (e) {
                e.preventDefault();
                $("#print_div").printThis({
                    header: $('#print_header'),
                    importStyle: true
                });
            });


            $("#ApproveBTN").click(function (e) {
                e.preventDefault();
                Swal.fire({
                    title: 'আপনি কি গৃহকরদাতার অননুমোদিত তথ্য যাচাই করে তা অনুমোদন করতে চান?',
                    text: 'একবার অনুমোদিত হয়ে গেলে পরবর্তীতে হালনাগাদ সম্ভব নয়',
                    icon: 'warning',
                    showCancelButton: true,
                    confirmButtonColor: '#3085d6',
                    cancelButtonColor: '#d33',
                    confirmButtonText: 'সাবমিট করুন',
                    cancelButtonText: 'বাতিল করুন'
                }).then((result) => {
                    if (result.value) {
                        $.ajax({
                            type: 'GET',
                            url: '/Holding/ApproveInformation',
                            data: "HolderId=" + @Model.HolderId,
                            contentType: 'application/json',
                            success: function (status) {

                                if (status === 202) {
                                    Swal.fire({
                                        text: "সফলভাবে গৃহকর অনুমোদন চূড়ান্তকরণ করা হয়েছে",
                                        icon: "success",
                                        buttonsStyling: false,
                                        confirmButtonText: "সফলভাবে সম্পন্ন",
                                        customClass: {
                                            confirmButton: "btn font-weight-bold btn-success",
                                        }
                                    });
                                    setTimeout(function () { window.location.href = "/HoldingTax/Index"; }, 2000);
                                }
                                else if (status === 409) {
                                    Swal.fire({
                                        text: " নির্বাচিত আর্থিক বছরের জন্য এখনো কোনো গৃহকর প্রস্তুত কর হয়নি",
                                        icon: "error",
                                        buttonsStyling: false,
                                        confirmButtonText: "বাতিল করা হয়েছে",
                                        customClass: {
                                            confirmButton: "btn font-weight-bold btn-danger",
                                        }
                                    });
                                    KTUtil.scrollTop();
                                }
                                else {
                                    Swal.fire({
                                        text: status,
                                        icon: "error",
                                        buttonsStyling: false,
                                        confirmButtonText: " বাতিল করা হয়েছে ",
                                        customClass: {
                                            confirmButton: "btn font-weight-bold btn-danger",
                                        }
                                    });
                                }
                            },
                            error: function (error) {
                                console.log(error);
                            }
                        });
                    }
                });
            });
        });

    </script>
}


