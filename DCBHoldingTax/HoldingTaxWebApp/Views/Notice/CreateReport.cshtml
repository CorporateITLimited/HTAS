@model HoldingTaxWebApp.Models.Holding.Notice
@using HoldingTaxWebApp.Helpers
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Sheets{
    <link href="~/Content/FormDesign.css" rel="stylesheet" />
}
<div class="subheader py-2 py-lg-6 subheader-transparent" id="kt_subheader">
    <div class="container d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
        <div class="d-flex align-items-center flex-wrap mr-1">
            <div class="d-flex align-items-baseline flex-wrap mr-5">
                <h5 class="text-dark font-weight-bold my-1 mr-5">গৃহকর সংক্রান্ত বিজ্ঞপ্তি </h5>
                <ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold p-0 my-2 font-size-sm">
                    <li class="breadcrumb-item text-muted">
                        <a href="#" class="text-muted">গৃহকর সংক্রান্ত নতুন বিজ্ঞপ্তি তৈরী</a>
                    </li>
                </ul>
            </div>
        </div>
        <div class="d-flex align-items-center">
            <a onclick="return goBack();" class="btn btn-light-primary font-weight-bolder btn-sm"><i class="fas fa-arrow-left"></i>আগের পেজে যান</a>
        </div>
    </div>
</div>
<div class="d-flex flex-column-fluid">
    <div class="container">
        @if (TempData["SM"] != null)
        {
            <div class="alert alert-custom alert-white alert-shadow gutter-b fade show mb-5" role="alert">
                <div class="alert-text font-weight-bolder" style="color: #0BB783;">@TempData["SM"]</div>
                <div class="alert-close">
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true"><i class="ki ki-close"></i></span>
                    </button>
                </div>
            </div>
        }
        <br />
        @if (Convert.ToInt32(Session["NoticeTypeId"].ToString()) == 3)
        {
            <div class="card card-custom">
                <div style="justify-content:center;text-align:center; overflow-y:hidden;">
                    <iframe runat="server" id="Reports" src="~/WebForms/Tax/leterThree.aspx"
                            style="width:100%; min-height:1000px;" frameBorder="0"></iframe>
                </div>
            </div>
        }
        else
        {
            <div class="card card-custom">
                <div style="justify-content:center;text-align:center; overflow-y:hidden;">
                    <iframe runat="server" id="Reports" src="~/WebForms/Tax/letterOne.aspx"
                            style="width:100%; min-height:1000px;" frameBorder="0"></iframe>
                </div>
            </div>
        }
    </div>
</div>







