@model HoldingTaxWebApp.Models.Tax.HoldingTax

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="subheader py-2 py-lg-6 subheader-transparent" id="kt_subheader">
    <div class="container d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
        <div class="d-flex align-items-center flex-wrap mr-1">
            <div class="d-flex align-items-baseline flex-wrap mr-5">
                <h5 class="text-dark font-weight-bold my-1 mr-5">গৃহকরের তথ্য</h5>
                <ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold p-0 my-2 font-size-sm">
                    <li class="breadcrumb-item text-muted">
                        @*<a href="#" class="text-muted">গৃহকরের তথ্য পরিবর্তন</a>*@
                    </li>
                </ul>
            </div>
        </div>
        <div class="d-flex align-items-center">
            <a onclick="return goBack();" class="btn btn-light-primary font-weight-bolder btn-sm"><i class="fas fa-arrow-left"></i>Back</a>
        </div>
    </div>
</div>

<div class="d-flex flex-column-fluid">
    <div class="container">
        <div class="row">
            <div class="col-md-12">

                <div class="card card-custom gutter-b example example-compact">
                    <div class="card-header">
                        <h3 class="card-title">গৃহকরের তথ্য পরিবর্তন</h3>
                        <div class="card-toolbar">
                            <div class="example-tools justify-content-center">
                               
                            </div>
                        </div>
                    </div>
                    <!--begin::Form-->
                    <form class="form">
                        <div class="card-body">
                            <div class="form-group row">
                                <label class="col-lg-2 col-form-label text-lg-right">Rebate:</label>
                                <div class="col-lg-1">
                                    <div class="radio-inline">
                                        <label class="radio radio-solid">
                                            <input type="radio" name="example_2" checked="checked" value="2">
                                            <span></span>YES
                                        </label>
                                        <label class="radio radio-solid">
                                            <input type="radio" name="example_2" value="2">
                                            <span></span>NO
                                        </label>
                                    </div>
                                    @*<span class="form-text text-muted">Please select user group</span>*@
                                </div>
                                <div class="col-lg-3">
                                    <input type="number" class="form-control" readonly>
                                </div>

                                <label class="col-lg-2 col-form-label text-lg-right">Is Finalised:</label>
                                <div class="col-lg-1">
                                    <div class="radio-inline">
                                        <label class="radio radio-solid">
                                            <input type="radio" name="example_2" checked="checked" value="2">
                                            <span></span>YES
                                        </label>
                                        <label class="radio radio-solid">
                                            <input type="radio" name="example_2" value="2">
                                            <span></span>NO
                                        </label>
                                    </div>
                                </div>

                            </div>

                            <div class="form-group row">
                                <label class="col-lg-2 col-form-label text-lg-right">Wrong Information Charge:</label>
                                <div class="col-lg-1">
                                    <div class="radio-inline">
                                        <label class="radio radio-solid">
                                            <input type="radio" name="example_2" checked="checked" value="2">
                                            <span></span>YES
                                        </label>
                                        <label class="radio radio-solid">
                                            <input type="radio" name="example_2" value="2">
                                            <span></span>NO
                                        </label>
                                    </div>
                                </div>
                                <div class="col-lg-3">
                                    <input type="number" class="form-control" readonly>
                                </div>

                            </div>

                            <div class="form-group row">
                                <label class="col-lg-3 col-form-label text-lg-right">Paid Amount:</label>
                                <div class="col-lg-3">
                                    <input type="number" class="form-control" placeholder="Enter Paid Amount">
                                    @*<span class="form-text text-muted">Please enter your full name</span>*@
                                </div>
                                <label class="col-lg-2 col-form-label text-lg-right">Net Payable Tax Amount:</label>
                                <div class="col-lg-3">
                                    <input type="number" class="form-control" placeholder="Enter Net Payable Tax Amount">
                                    @*<span class="form-text text-muted">Please enter your contact number</span>*@
                                </div>
                            </div>

                        </div>
                        <div class="card-footer">
                            <div class="row">
                                <div class="col-lg-5"></div>
                                <div class="col-lg-3">
                                    <button type="reset" class="btn btn-success mr-2">Submit</button>
                                    <button type="reset" class="btn btn-secondary">Cancel</button>
                                </div>
                                <div class="col-lg-4"></div>
                            </div>
                        </div>
                    </form>
                    <!--end::Form-->
                </div>

            </div>
        </div>
    </div>
</div>